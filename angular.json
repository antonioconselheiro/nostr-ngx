{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nostr-credential-ngx": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/nostr-credential-ngx",
      "sourceRoot": "projects/nostr-credential-ngx/src",
      "prefix": "nostr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/nostr-credential-ngx/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/nostr-credential-ngx/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/nostr-credential-ngx/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/nostr-credential-ngx/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "**/*.ts",
              "**/*.html"
            ]
          }
        }
      }
    },
    "nostr-ngx": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/nostr-ngx",
      "sourceRoot": "projects/nostr-ngx/src",
      "prefix": "nostr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/nostr-ngx/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/nostr-ngx/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/nostr-ngx/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/nostr-ngx/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "**/*.ts",
              "**/*.html"
            ]
          }
        }
      }
    },
    "belomonte": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/belomonte",
      "sourceRoot": "projects/belomonte/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/belomonte",
            "index": "projects/belomonte/src/index.html",
            "browser": "projects/belomonte/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/belomonte/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/belomonte/src/favicon.ico",
              "projects/belomonte/src/assets"
            ],
            "styles": [
              "dist/nostr-credential-ngx/styles.scss",
              "projects/belomonte/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "belomonte:build:production"
            },
            "development": {
              "buildTarget": "belomonte:build:development",
              "port": 4203
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "belomonte:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/belomonte/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/belomonte/src/favicon.ico",
              "projects/belomonte/src/assets"
            ],
            "styles": [
              "projects/belomonte/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "**/*.ts",
              "**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
