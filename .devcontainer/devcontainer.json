{
  "name": "Nostr for Angular",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "nostr-ngx-dev",
	"workspaceFolder": "/nostr-ngx",
	"customizations": {
		"vscode": {
			"extensions": [
        "Angular.ng-template",
        "ms-vscode.vscode-typescript-next",
        "dbaeumer.vscode-eslint",
        "eamodio.gitlens",
        "naumovs.color-highlight",
        "manishsencha.readme-preview",
        "wix.vscode-import-cost",
        "jrjrjr.amberlight",
        "natewallace.angular2-inline"
      ]
		}
	}
}
